# Hassio (remote)
# useage: ssh hassio-remote
# Before running this command, run a socks4 proxy on port 1080
# to a local machine
Host hassio-remote
Hostname 10.10.1.70
User root
IdentityFile ~/.dan/.ssh/id_rsa.barbecue
ProxyCommand /usr/bin/nc -X 4 -x 127.0.0.1:1080 %h %p


